# API Keys for Historical Stock Data
# Copy this to .env and fill in your keys

# Massive.com API
MASSIVE_API_KEY=your_api_key_here
MASSIVE_S3_ACCESS_KEY=your_access_key_here
MASSIVE_S3_SECRET_KEY=your_secret_key_here
MASSIVE_S3_ENDPOINT=https://files.massive.com
MASSIVE_S3_BUCKET=flatfiles

# Alpha Vantage API
ALPHA_VANTAGE_API_KEY=your_api_key_here

# SEC EDGAR (no key required, but rate limit applies)
SEC_EDGAR_USER_AGENT=YourApp/1.0 (your@email.com)

# Local data paths
PENNY_STOCKS_DATA_PATH=../Penny-Stocks-Market-Data
